<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="paperformat_move_report" model="report.paperformat">
        <field name="name">European A4 low margin</field>
        <field name="default" eval="True"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">15</field>
        <field name="margin_bottom">5</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">15</field>
        <field name="dpi">80</field>
    </record>

    <record id="report_account_move" model="ir.actions.report">
        <field name="name">Print Accounting Entry</field>
        <field name="model">account.move</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">l10n_ir.account_move_report</field>
        <field name="report_file">l10n_ir.account_move_report</field>
        <field name="paperformat_id" ref="l10n_ir.paperformat_move_report"/>
        <field name="binding_model_id" ref="account.model_account_move"/>
        <field name="binding_type">report</field>
    </record>
    <record id="report_account_move_less_detail" model="ir.actions.report">
        <field name="name">Print Accounting Entry Less details</field>
        <field name="model">account.move</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">l10n_ir.account_move_report_less_detail</field>
        <field name="report_file">l10n_ir.account_move_report_less_detail</field>
        <field name="paperformat_id" ref="l10n_ir.paperformat_move_report"/>
        <field name="binding_model_id" ref="account.model_account_move"/>
        <field name="binding_type">report</field>
    </record>

    <record id="paperformat_invoice" model="report.paperformat">
        <field name="name">Invoice A4</field>
        <field name="default" eval="True"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">6.5</field>
        <field name="margin_bottom">6.5</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">0</field>
        <field name="dpi">98</field>
    </record>

    <record id="action_invoice_official_with_table" model="ir.actions.report">
        <field name="model">account.move</field>
        <field name="name">Official Invoice With Table</field>
        <field name="report_name">l10n_ir.report_invoice_official_with_table</field>
        <field name="report_file">l10n_ir.report_invoice_official_with_table</field>
        <field name="report_type">qweb-pdf</field>
        <field name="attachment">(object.state == 'posted') and ((object.name or 'INV').replace('/','_')+'.pdf')</field>
        <field name="paperformat_id" ref="paperformat_invoice"/>
        <field name="print_report_name">'Official Invoice With Table - %s' %(object.name)</field>
        <field name="binding_model_id" ref="account.model_account_move"/>
    </record>

    <record id="action_invoice_official_with_table_print" model="ir.actions.report">
        <field name="model">res.company</field>
        <field name="name">Invoice Paper Print</field>
        <field name="report_name">l10n_ir.invoice_official_with_table_print</field>
        <field name="report_file">l10n_ir.invoice_official_with_table_print</field>
        <field name="report_type">qweb-pdf</field>
        <field name="paperformat_id" ref="paperformat_invoice"/>
        <field name="print_report_name">'Invoice Paper Print - %s' % (object.name)</field>
        <field name="binding_model_id" ref="base.model_res_company"/>
    </record>

    <record id="account.account_invoices_without_payment" model="ir.actions.report">
        <field name="binding_model_id"/>
    </record>

    <record id="account.account_invoices" model="ir.actions.report">
        <field name="binding_model_id"/>
    </record>

</odoo>